#!/bin/sh
# postinst script for ahenk-system-restriction
#
# see: dh_installdeb(1)

set -e


case "$1" in
    configure)

        #reload plugin

    ;;

    abort-upgrade|abort-remove|abort-deconfigure)
    ;;

    *)
        echo "postinst called with unknown argument \`$1'" >&2
        exit 1
    ;;
esac

exit 0
